{
  "name": "maris/symfony-suggestions-bundle",
  "description": "Пакет добавляет для работы с списками для npm библиотеки suggestions-jquery.",
  "license": "MIT",
  "version": "0.0.1",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Марисов Николай Андреевич",
      "email": "marisov123456789@mail.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "symfony/config": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/http-kernel": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Maris\\Symfony\\Suggestions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maris\\Symfony\\Suggestions\\Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  }
}